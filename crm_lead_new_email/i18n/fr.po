#. module: crm_lead_new_email
#: model:mail.template,body_html:crm_lead_new_email.email_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p>Message from ${object.email_from}</p>\n"
"                % if not object.partner_id:\n"
"                <p style=\"color: rgb(231, 99, 99);\">No contact matches were found for this email.<br/>\n"
"                It is advisable to create a contact associated with this email to enable automatic recognition of incoming messages.\n"
"                </p>\n"
"                % endif\n"
"                <a href=\"${'/mail/view?model=%s\\u0026res_id=%s' % ('crm.lead', object.id)}\" target=\"_blank\" style=\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px;\">Open in Odoo</a>\n"
"                % if object.original_message_id.body:\n"
"                <p>\n"
"                ${object.original_message_id.body|safe}\n"
"                </p>\n"
"                % endif\n"
"            </div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p>Message de ${object.email_from}</p>\n"
"                % if not object.partner_id:\n"
"                <p style=\"color: rgb(231, 99, 99);\">Aucune correspondance de contact n'a été trouvé pour cet e-mail.<br/>\n"
"                Il est conseillé de créer un contact associé à cet e-mail pour activer la reconnaissance automatique des messages entrants.\n"
"                </p>\n"
"                % endif\n"
"                <a href=\"${'/mail/view?model=%s\\u0026res_id=%s' % ('crm.lead', object.id)}\" target=\"_blank\" style=\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px;\">Ouvrir dans Odoo</a>\n"
"                % if object.original_message_id.body:\n"
"                <p>\n"
"                ${object.original_message_id.body|safe}\n"
"                </p>\n"
"                % endif\n"
"            </div>\n"
"        "

