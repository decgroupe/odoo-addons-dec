<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="ir.ui.view" id="view_view_custom_form">
        <field name="name">res.city.zip.form@base_dashboard_edit</field>
        <field name="model">ir.ui.view.custom</field>
        <field name="inherit_id" ref="base.view_view_custom_form" />
        <field name="arch" type="xml">
            <field name="ref_id" position="after">
                <field name="create_date" />
            </field>
        </field>
    </record>

    <record id="view_view_custom_tree" model="ir.ui.view">
        <field name="name">res.city.zip.tree@base_dashboard_edit</field>
        <field name="model">ir.ui.view.custom</field>
        <field name="inherit_id" ref="base.view_view_custom_tree" />
        <field name="arch" type="xml">
            <field name="ref_id" position="after">
                <field name="create_date" />
            </field>
        </field>
    </record>

    <record id="view_view_custom_search" model="ir.ui.view">
        <field name="name">res.city.zip.search@base_dashboard_edit</field>
        <field name="model">ir.ui.view.custom</field>
        <field name="inherit_id" ref="base.view_view_custom_search" />
        <field name="arch" type="xml">
            <data />
        </field>
    </record>

