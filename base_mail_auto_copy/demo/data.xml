<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="mail.template" id="test_template">
        <field name="name">My Template Name</field>
        <field name="model_id" ref="base.model_res_partner" />
        <field name="email_from">${object.email}</field>
        <field name="email_to">fake@domain.com</field>
        <field name="subject">My Template Subject</field>
        <field name="auto_delete" eval="False" />
        <field name="lang">${object.lang}</field>
        <field
            name="body_html"
        ><![CDATA[<html>
                <head></head>
                <body>
                    <p>Hello World</p>
                </body>
            </html>
        ]]></field>
    </record>

</odoo>
