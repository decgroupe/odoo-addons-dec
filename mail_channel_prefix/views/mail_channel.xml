<?xml version="1.0" ?>
<odoo>

    <record model="ir.ui.view" id="mail_channel_view_form">
        <field name="name">mail.channel.form@mail_channel_prefix</field>
        <field name="model">mail.channel</field>
        <field name="inherit_id" ref="mail.mail_channel_view_form" />
        <field name="arch" type="xml">
            <field name="email_send" position="after">
                <field name="subject_prefix" attrs="{'invisible': []}" />
            </field>
        </field>
    </record>

</odoo>
