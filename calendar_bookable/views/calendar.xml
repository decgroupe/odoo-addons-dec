<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="ir.ui.view" id="view_calendar_event_search">
        <field name="name">calendar.event.search@calendar_reservable</field>
        <field name="model">calendar.event</field>
        <field name="inherit_id" ref="calendar.view_calendar_event_search" />
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='mymeetings']" position="after">
                <filter
                    string="Bookable"
                    help="Bookables like vehicles, rooms, etc."
                    name="filter_bookable"
                    domain="[('partner_ids.bookable', '=', True)]"
                />
            </xpath>
        </field>
    </record>

    <record model="ir.actions.act_window" id="calendar.action_calendar_event">
        <field
            name="context"
        >{'search_default_mymeetings': 1, 'search_default_filter_bookable': 1}</field>
    </record>

</odoo>
