<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="ir.ui.view" id="view_move_tree_view">
        <field name="name">account.move.tree@account_traceability</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_tree" />
        <field name="arch" type="xml">
            <field name="ref" position="after">
                <field name="invoice_origin" optional="hide"/>
            </field>
        </field>
    </record>

    <record model="ir.ui.view" id="account_move_filter_view">
        <field name="name">account.move.select@account_traceability</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_account_move_filter" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="invoice_origin"/>
            </field>
        </field>
    </record>

</odoo>
