<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record model="ir.ui.view" id="hr_timesheet_line_calendar">
        <field
            name="name"
        >account.analytic.line.calendar@helpdesk_timesheet_calendar</field>
        <field name="model">account.analytic.line</field>
        <field
            name="inherit_id"
            ref="hr_timesheet_calendar.hr_timesheet_line_calendar"
        />
        <field name="arch" type="xml">
            <xpath expr="//calendar" position="inside">
                <field name="ticket_id" />
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="hr_timesheet_line_calendar_all">
        <field
            name="name"
        >account.analytic.line.calendar@helpdesk_timesheet_calendar_all</field>
        <field name="model">account.analytic.line</field>
        <field
            name="inherit_id"
            ref="hr_timesheet_calendar.hr_timesheet_line_calendar_all"
        />
        <field name="arch" type="xml">
            <xpath expr="//calendar" position="inside">
                <field name="ticket_id" />
            </xpath>
        </field>
    </record>
</odoo>
