<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record model="ir.ui.view" id="hr_timesheet_line_form_view">
        <field
            name="name"
        >account.analytic.line.form@crm_timesheet_identification</field>
        <field name="model">account.analytic.line</field>
        <field
            name="inherit_id"
            ref="crm_timesheet_lead_context.hr_timesheet_line_form_view"
        />
        <field name="priority">25</field>
        <field name="arch" type="xml">
            <field name="lead_id" position="after">
                <field
                    name="lead_identification"
                    attrs="{'invisible': [('lead_id', '=', False)]}"
                    string=""
                />
            </field>
        </field>
    </record>

</odoo>
