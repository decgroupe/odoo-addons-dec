<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.ui.view" id="software_license_application_form_view" >
        <field name="name">software.license.application.form@software_license_keygen</field>
        <field name="model">software.license.application</field>
        <field name="inherit_id" ref="software_license.software_license_application_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='licensing']/group[@name='main']/group[@name='secondary']" position="inside">
                <field name="auto_generate_serial"/>
            </xpath>
        </field>
    </record>

</odoo>
