<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <data noupdate="1">
        <record id="procurement_exception_default_rule" model="procurement.exception">
            <field
                name="name"
            >Default rule to redirect every exception to admin user</field>
            <field name="user_id" ref="base.user_admin" />
            <field name="regex_pattern">.*</field>
            <field name="sequence" eval="10" />
        </record>
    </data>

</odoo>
