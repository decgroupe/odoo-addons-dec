<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <data noupdate="1">
        <record id="sequence_mrp_route" model="ir.sequence">
            <field name="name">Application Pass</field>
            <field name="code">software.license.pass</field>
            <field name="prefix">AP/%(y)s</field>
            <field name="padding">4</field>
            <field name="number_first">100</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True" />
            <field name="range_reset">yearly</field>
        </record>
    </data>

</odoo>
