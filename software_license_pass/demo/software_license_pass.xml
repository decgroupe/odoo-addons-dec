        name="action_resync_with_pack"
        eval="[[
            ref('software_license_pass.pass_basic1'),
            ref('software_license_pass.pass_basic2'),
            ref('software_license_pass.pass_mid1'),
            ref('software_license_pass.pass_premium1'),
            ref('software_license_pass.pass_premium2'),
            ref('software_license_pass.pass_premium3'),
        ]]"
    />

    <function
        model="software.license.pass"
        name="activate"
        eval="[[ref('software_license_pass.pass_mid1'),]] "
    >
        <value name="hardware">3a006cbacfb50</value>
        <value name="info">
            {
                "httprequest": {
                    "remote_addr": "98.89.72.2"
                },
                "telemetry": {
                    "NetworkInformation": {
                        "DomainName": "ad.lumberinc.com",
